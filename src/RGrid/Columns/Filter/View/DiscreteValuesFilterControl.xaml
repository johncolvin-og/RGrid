<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:c="clr-namespace:mbts.ui.controls"
                    xmlns:usp="clr-namespace:mbts.ui.spark">
  <ResourceDictionary.MergedDictionaries>
    <ResourceDictionary Source="/view/Controls/ComboCheckBox/ComboCheckBox.xaml"/>
  </ResourceDictionary.MergedDictionaries>
  <Style TargetType="usp:DiscreteValuesFilterControl">
    <Setter Property="Template">
      <Setter.Value>
		  <ControlTemplate TargetType="usp:DiscreteValuesFilterControl">
			 <ListBox x:Name="PART_ListBox" ItemsSource="{Binding ItemsSource, RelativeSource={RelativeSource TemplatedParent}}"/>
		  </ControlTemplate>
		</Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>